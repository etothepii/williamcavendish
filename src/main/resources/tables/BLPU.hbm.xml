<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.co.epii.conservatives.williamcavendishbentinck.tables.BLPU" table="BLPU">
        <id name="uprn" type="long" column="UPRN">
            <generator class="native"/>
        </id>
        <property name="recordIdentifier" column="RECORD_IDENTIFIER" type="integer" />
        <property name="changeType" column="CHANGE_TYPE" type="string" />
        <property name="proOrder" column="PRO_ORDER" type="long" />
        <property name="logicalStatus" column="LOGICAL_STATUS" type="integer" />
        <property name="blpuState" column="BLPU_STATE" type="integer" />
        <property name="blpuStateDate" column="BLPU_STATE_DATE" type="date" />
        <property name="parentUprn" column="PARENT_UPRN" type="long" />
        <property name="XCoordinate" column="X_COORDINATE" type="float" />
        <property name="YCoordinate" column="Y_COORDINATE" type="float" />
        <property name="rpc" column="RPC" type="integer" />
        <property name="localCustodianCode" column="LOCAL_CUSTODIAN_CODE" type="integer" />
        <property name="startDate" column="START_DATE" type="date" />
        <property name="endDate" column="END_DATE" type="date" />
        <property name="lastUpdateDate" column="LAST_UPDATE_DATE" type="date" />
        <property name="entryDate" column="ENTRY_DATE" type="date" />
        <property name="postalAddress" column="POSTAL_ADDRESS" type="string" />
        <property name="postcodeLocator" column="POSTCODE_LOCATOR" type="string" />
        <property name="multiOccCount" column="MULTI_OCC_COUNT" type="integer" />
    </class>
</hibernate-mapping>
