<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.co.epii.politics.williamcavendishbentinck.tables.BoundedArea" table="BoundedArea">
        <id name="id" type="integer" column="ID">
            <generator class="assigned"/>
        </id>
        <property name="parent" column="PARENT" type="integer" />
        <property name="owner" column="OWNER" type="integer" />
        <property name="ownerGroup" column="OWNER_GROUP" type="integer" />
        <property name="name" column="NAME" type="string" />
        <property name="boundary" column="BOUNDARY" type="blob" />
    </class>
</hibernate-mapping>
