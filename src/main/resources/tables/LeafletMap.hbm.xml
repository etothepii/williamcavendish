<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.co.epii.politics.williamcavendishbentinck.tables.LeafletMap" table="LeafletMap">
        <id name="id" type="string" column="ID">
            <generator class="assigned"/>
        </id>
        <property name="leaflet" column="LEAFLET" type="integer" />
        <property name="route" column="ROUTE" type="integer" />
        <property name="deliveredBy" column="DELIVERED_BY" type="integer" />
    </class>
</hibernate-mapping>
